<?xml version="1.0" ?>
<benchmark tool="dummy">
<!-- Example file for benchmark definition for BenchExec
     with dummy tool that takes several properties in the property file
     and uses multi-property verification.
     Correct results are placed in options. -->
  <rundefinition name="multiproperty">
    <option>Property unreach-call(error_function_1): false(unreach-call)</option>
    <option>Property valid-free: false(valid-free)</option>
    <option>Property unreach-call(error_function_4): false(unreach-call)</option>
    <option>Property no-deadlock: false(no-deadlock)</option>
    <option>Property unreach-call(error_function_5): true</option>
    <option>Property unreach-call(error_function_2): true</option>
    <option>Property valid-deref: true</option>
    <option>Property no-overflow: true</option>
    <option>Property unreach-call(error_function_3): true</option>
    <option>Property termination: true</option>
    <option>false(unreach-call)</option>
  </rundefinition>
  
  <propertyfile kind="multiproperty">properties/multiproperty-example.prp</propertyfile>

  <tasks name="Dummy multiproperty task">
    <include>dummy_false-unreach-call.yml</include>
  </tasks>
</benchmark>
